package bug;

// ispisuje tablicu mno≈æenja od 1 do 15 u redove i kolone

public class TablicaMnozenja {
	public static void main(String[] args) {
		for (int x = 0; x < 15; x++) {
			for (int y = 0; y < 15; y++) {
				System.out.printf("%3d ", (x + 1) * (y + 1));
			}
			System.out.println();
		}
	}
}
